	pages = {16--31},
	file = {IEEE Xplore Abstract Record:/Users/jravan/Zotero/storage/9ENZ22TF/485626.html:text/html;Thomas and Sandhu - 1996 - A trusted subject architecture for multilevel secu.pdf:/Users/jravan/Zotero/storage/RTQNKTWQ/Thomas and Sandhu - 1996 - A trusted subject architecture for multilevel secu.pdf:application/pdf}
