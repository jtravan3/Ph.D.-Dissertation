@inproceedings{ahmed_dynamic_2010,
	title = {Dynamic trust management based on routing system},
	doi = {10.1109/ICCTD.2010.5645861},
	abstract = {The distributed trust management plays an important role in the interactions, and has challenges in dynamic reputation evaluation, calculation and deployment due to lack of central control. The paper gives a method for the link reputation evaluation and the reputation calculation for node, path and end-to-end. The local link reputations are broadcasted by the OSPF routing protocol, which updates link reputations dynamically. The trust policies are applied to link reputations, form the dedicated reputation forwarding table, and forward packet over secure and trusted links, nodes and paths. Comparing with static trust management, the dynamic trust management can meet the requirement of real time reputation evaluation, broadcasting and updating.},
